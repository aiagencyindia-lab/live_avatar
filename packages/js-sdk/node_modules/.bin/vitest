#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/madhavjangid/Downloads/Shubham Work/live_avatar/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.7_happy-dom@20.4.0_jiti@1.21.7_yaml@2.8.2/node_modules/vitest/node_modules:/Users/madhavjangid/Downloads/Shubham Work/live_avatar/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.7_happy-dom@20.4.0_jiti@1.21.7_yaml@2.8.2/node_modules:/Users/madhavjangid/Downloads/Shubham Work/live_avatar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/madhavjangid/Downloads/Shubham Work/live_avatar/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.7_happy-dom@20.4.0_jiti@1.21.7_yaml@2.8.2/node_modules/vitest/node_modules:/Users/madhavjangid/Downloads/Shubham Work/live_avatar/node_modules/.pnpm/vitest@4.0.18_@types+node@22.19.7_happy-dom@20.4.0_jiti@1.21.7_yaml@2.8.2/node_modules:/Users/madhavjangid/Downloads/Shubham Work/live_avatar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
